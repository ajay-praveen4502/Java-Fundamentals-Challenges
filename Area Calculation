import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        // initializing the side, length, breath and radius of the circle and their area as well
        int side, length, breadth, square, rectangle;
        float radius, circle;
        
        // creating an object for the Scanner function
        Scanner sc = new Scanner(System.in);
        
        //getting user input
        side = sc.nextInt();
        length = sc.nextInt();
        breadth = sc.nextInt();
        radius = sc.nextFloat();
        
        //calculation of area of square
        square = side * side;
        System.out.println(square);
        
        //calculation of area of rectangle
        rectangle = length * breadth;
        System.out.println(rectangle);
        
        //calculation of area of circle
        circle = 3.14f * (radius * radius);
        System.out.print(circle);
    }
}
