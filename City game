import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);//creating an object for scanner class
		
		//getting user input and initializin them
		System.out.println("Enter the number of levels");
		int num_of_levels = sc.nextInt();
		System.out.println("Enter the required sum of points");
		int req_sum_of_points = sc.nextInt();
		
		if (req_sum_of_points % num_of_levels == 0){	//
		    System.out.print("The minimum number of levels is "+ num_of_levels);
		}
		else{
		    System.out.print("The minimum number of levels is "+(req_sum_of_points%num_of_levels));
		}
	}
}
